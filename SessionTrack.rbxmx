local Logger = {
    Tags = {
        DayToday = "TODAYSDATE",
        SessionTime = "SESSIONTIME",
        TotalTime = "TOTALTIME",
        RawSessionTime = "RAWSESSIONTIME",
        RawTotalTime = "RAWTOTALTIME",
        SessionState = "SESSIONSTATE",
    }
}

Logger.__index = Logger

